{
    "name": "tecsiaron/anaf-api-client-php",
    "description": "PHP API Client for ANAF API and Ro eFactura",
    "keywords": [
        "anaf",
        "api",
        "cif",
        "api client",
        "roefactura",
        "efactura"
    ],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "league/oauth2-client": "^2.7",
        "php": ">=8.0",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "EdituraEDU\\ANAF\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdituraEDU\\ANAF\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tecsi Aron",
            "email": "aron@edituraedu.ro"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^11"
    }
}
